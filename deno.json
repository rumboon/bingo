{
  "version": "0.0.1",
  "nodeModulesDir": "auto",
  "imports": {
    "@eslint/js": "npm:@eslint/js@9.39.1",
    "@std/http": "jsr:@std/http@^1.0.23",
    "@types/node": "npm:@types/node@24.10.1",
    "@types/react": "npm:@types/react@19.2.5",
    "@types/react-dom": "npm:@types/react-dom@19.2.3",
    "react": "npm:react@19.1.0",
    "react-dom": "npm:react-dom@19.1.0",
    "eslint": "npm:eslint@9.39.1",
    "eslint-plugin-react-hooks": "npm:eslint-plugin-react-hooks@7.0.1",
    "eslint-plugin-react-refresh": "npm:eslint-plugin-react-refresh@0.4.24",
    "globals": "npm:globals@16.5.0",
    "typescript": "npm:typescript@5.9.3",
    "typescript-eslint": "npm:typescript-eslint@8.46.4",
    "vite": "npm:vite@6.3.5"
  },
  "tasks": {
    "dev": "deno run -A npm:vite@6.3.5",
    "build": "deno run -A npm:vite@6.3.5 build",
    "lint": "deno run -A npm:eslint@9.39.1 .",
    "preview": "deno run -A npm:vite@6.3.5 preview",
    "start": "deno run -A main.ts"
  },
  "deploy": {
    "org": "rumboon",
    "app": "bingo",
    "exclude": ["**/node_modules"],
    "entrypoint": "main.ts"
  }
}
